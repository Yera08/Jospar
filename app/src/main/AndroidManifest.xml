<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/img"
        android:label="@string/app_name"
        android:roundIcon="@drawable/img"
        android:supportsRtl="true"
        android:theme="@style/Theme.Jospar"
        tools:targetApi="31">
        <activity
            android:name=".MenuActivities.FourMonthPlanActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name=".MenuActivities.TenYearGoalsActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name=".MenuActivities.TechAssistActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name=".MenuActivities.OneYearGoalsActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name=".MenuActivities.FiveYearGoalsActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name=".Activities.SplashActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.MenuActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name=".Activities.ToDoActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name=".Activities.MainActivity"
            android:screenOrientation="portrait"
            android:exported="true">
        </activity>
    </application>

</manifest>